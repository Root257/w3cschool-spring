<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean id="helloWorld" class="com.w3cschool.HelloWorld" >
    <property name="message1" value="Hello World 1"/>
    <property name="message2" value="Hello World 2"/>
</bean>
    <bean class="com.w3cschool.InitHelloWorld"></bean>
    <bean id="helloIndia" class="com.w3cschool.HelloIndia" parent="helloWorld"></bean>
</beans>